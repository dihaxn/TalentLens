{
  "name": "web",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "root": "Frontend/apps/web",
  "sourceRoot": "Frontend/apps/web/src",
  "projectType": "application",
  "tags": [],
  "// targets": "to see all targets run: nx show project web --web",
  "targets": {
    "dev": {
      "executor": "@nx/vite:dev-server",
      "options": {
  "configFile": "Frontend/apps/web/vite.config.ts",
  "buildTarget": "build"
      }
    },
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/web",
        "configFile": "Frontend/apps/web/vite.config.ts"
      }
    },
    "preview": {
      "executor": "@nx/vite:preview",
      "options": {
        "buildTarget": "build"
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["{workspaceRoot}/coverage/web"],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "coverage/web"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "Frontend/apps/web/**/*.{ts,tsx,js,jsx}"
        ]
      }
    }
  }
}
