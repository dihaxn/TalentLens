{
  "name": "shared-ui",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "root": "Frontend/libs/shared-ui",
  "sourceRoot": "Frontend/libs/shared-ui/src",
  "projectType": "library",
  "tags": [],
  "// targets": "to see all targets run: nx show project shared-ui --web",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/shared-ui",
        "main": "src/index.ts",
        "tsConfig": "tsconfig.lib.json",
        "assets": ["*.md"]
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "Frontend/libs/shared-ui/**/*.{ts,tsx,js,jsx}"
        ]
      }
    }
  }
}
