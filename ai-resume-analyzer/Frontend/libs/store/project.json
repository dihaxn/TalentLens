{
  "name": "store",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "root": "Frontend/libs/store",
  "sourceRoot": "Frontend/libs/store/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
  "outputPath": "dist/libs/store",
  "main": "src/index.ts",
  "tsConfig": "tsconfig.lib.json",
  "assets": ["*.md"]
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "Frontend/libs/store/**/*.{ts,tsx,js,jsx}"
        ]
      }
    }
  }
}
